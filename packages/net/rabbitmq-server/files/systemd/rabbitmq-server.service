[Unit]
Description=RabbitMQ messaging service
After=network.target

[Service]
User=rabbitmq
WorkingDirectory=/var/lib/rabbitmq
ExecStart=/usr/sbin/rabbitmq-server

[Install]
WantedBy=multi-user.target
